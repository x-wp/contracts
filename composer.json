{
  "name": "x-wp/contracts",
  "description": "A set of abstractions for xWP components",
  "type": "library",
  "keywords": [
    "abstractions",
    "contracts",
    "decoupling",
    "interfaces",
    "interoperability",
    "standards",
    "extended-wp"
  ],
  "authors": [
    {
      "name": "Sibin Grasic",
      "email": "sibin.grasic@oblak.studio"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "oblak/wordpress-coding-standard": "^1"
  },
  "replace": {
    "x-wp/hook-contracts": "self.version"
  },
  "suggest": {
    "automattic/jetpack-autoloader": "Allow for better interoperability with other plugins that use this package."
  },
  "autoload": {
    "psr-4": {
      "XWP\\Contracts\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
